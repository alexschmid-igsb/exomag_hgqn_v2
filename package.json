    "scripts": {
        "inst": "./npm_reinstall.sh",
        "clean": "./clean.sh",
        "build": "cd frontend && react-scripts build",
        "dev": "concurrently --kill-others-on-fail \"yarn backend:dev\" \"yarn frontend:dev\"",
        "redis:start": "./backend/redis/start.sh",
        "redis:stop": "./backend/redis/stop.sh",
        "backend:dev": "./dev.sh",
        "backend:dev:execute": "INSTANCE_CONFIG_PATH=./config/exomag PROFILE=development_default nodemon backend/run",
        "frontend:dev": "cd frontend && react-scripts start",
        "seed:dev": "INSTANCE_CONFIG_PATH=./config/exomag PROFILE=development_default ./seed.sh",
        "seed:production": "INSTANCE_CONFIG_PATH=./config/exomag PROFILE=production_default ./seed.sh"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
